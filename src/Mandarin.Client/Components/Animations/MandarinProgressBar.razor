<Progress Size="Size.Medium">
  <ProgressBar Animated="true" Background="Background.Primary" Max="100" Min="0" Striped="true" Value="100">
    @ChildContent
  </ProgressBar>
</Progress>

@code
{

  /// <summary>
  /// Gets or sets the content to display within the button.
  /// </summary>
  [Parameter]
  public RenderFragment ChildContent { get; set; }

}
